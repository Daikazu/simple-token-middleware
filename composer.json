{
  "name": "daikazu/simple-token-middleware",
  "description": "A simple token middleware",
  "license": "MIT",
  "keywords": [
    "token",
    "middleware"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Mike Wall",
      "email": "daikazu@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.7"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Daikazu\\SimpleTokenMiddleware\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Daikazu\\SimpleTokenMiddleware\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daikazu\\SimpleTokenMiddleware\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
